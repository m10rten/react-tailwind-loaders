{
  "name": "react-tailwind-loaders",
  "version": "0.1.4",
  "description": "React.js loaders build with TailwindCSS, usable in any react or next application with tailwindcss.",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "watch": "tsc -w",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.{ts,tsx}",
    "prepublish": "npm run build",
    "prepublishOnly": "npm run lint && npm run build",
    "check:version": "npx @jetvil/cli version --package react-tailwind-loaders"
  },
  "keywords": [
    "react",
    "tailwindcss",
    "loaders",
    "react-loaders",
    "react-tailwind-loaders",
    "react-tailwindcss-loaders"
  ],
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=8.18.0"
  },
  "author": "m10rten",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
